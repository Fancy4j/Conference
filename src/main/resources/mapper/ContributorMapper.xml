<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.mapper.ContributorMapper">

    <!--新增一条投稿信息-->
    <insert id="addContribution" parameterType="com.example.pojo.UserArticle">
        insert into user_article (article_id,user_id,title,content,ctime,keywords,article_ref)
        values(#{articleId},#{userId},#{title},#{content},#{ctime},#{keywords},#{articleRef})
    </insert>

    <insert id="addMeetingArticle" parameterType="com.example.pojo.MeetingArticle">
        insert into meeting_article (article_id,meeting_id)
        values(#{articleId},#{meetingId})
    </insert>

    <insert id="addMeetingContributor" parameterType="com.example.pojo.ContributorMeeting">
        insert into contributor_meeting (meeting_id,user_id)
        values(#{meetingId},#{userId})
    </insert>
</mapper>